<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lock.myapplication">

    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.example.reapp.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/net"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name="com.example.reapp.AdminTestActivity"></activity>
        <activity android:name="com.example.reapp.DepartmentActivity"></activity>
        <activity android:name="com.example.reapp.MainActivity"></activity>
        <activity android:name="com.example.reapp.echartTest" />
        <activity android:name="com.example.reapp.nineUnlockActivity" />
        <activity android:name="com.example.reapp.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.reapp.UserLoginActivity"></activity>
        <activity android:name="com.example.reapp.UserRegisterActivity" />
    </application>

</manifest>